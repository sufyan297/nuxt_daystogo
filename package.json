{
  "name": "daystogo",
  "version": "1.0.0",
  "description": "Nuxt.js project",
  "author": "Mohammed Sufyan <sufyan297@gmail.com>",
  "private": true,
  "scripts": {
    "babel-build": "./node_modules/.bin/babel . --presets babel-preset-env,babel-preset-stage-2 --out-dir dist --ignore ./dist,./node_modules",
    "babel-watch": "./node_modules/.bin/babel . --presets babel-preset-env,babel-preset-stage-2 --watch --out-dir dist --ignore ./dist,./node_modules",
    "dev": "yarn babel-build && node dist/server.js",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@sufyan/chidiya": "^0.0.4",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.2.0",
    "isomorphic-fetch": "^2.2.1",
    "nuxt": "^1.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "eslint": "^4.15.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.0.0"
  }
}
